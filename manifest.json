{   
  "name": "YouMusic",
  "version": "0.2",
  "manifest_version": 2, 
  "description": "Make YouTube your music player",
  "icons": { 
    "16" : "images/icon.png"
  }, 
  "homepage_url": "",
  "permissions": [
    "http://youtube.com/*",
    "http://www.youtube.com/*",
    "tabs"
  ], 
  "content_scripts": [{
    "matches": ["http://*.youtube.com/*", "https://*.youtube.com/*"],
    "js": ["jquery.js", "youmusic.js"]
  }],
  "homepage_url": "http://netbaiji.com",
  "browser_action": {
    "default_icon": "images/icon.png",
    "default_title": "You Music",
    "default_popup": "popup.html"
  } 
} 
